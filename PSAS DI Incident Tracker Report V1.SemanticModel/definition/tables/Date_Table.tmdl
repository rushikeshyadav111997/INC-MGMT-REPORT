	column Latest3MonthsFlag = ```
			
			VAR LatestDate = MAX('Date_Table'[Date])  -- Get the latest date in the table
			VAR LatestMonth = MONTH(LatestDate)  -- Extract the month from the latest date
			VAR LatestYear = YEAR(LatestDate)  -- Extract the year from the latest date
			VAR CurrentMonth = MONTH('Date_Table'[Date])  -- Extract the month for each row in the Date Table
			VAR CurrentYear = YEAR('Date_Table'[Date])  -- Extract the year for each row in the Date Table
			VAR IsLatestMonth = (CurrentMonth = LatestMonth && CurrentYear = LatestYear)
			VAR IsPreviousMonth = (CurrentMonth = LatestMonth - 1 || (LatestMonth = 1 && CurrentMonth = 12) && CurrentYear = LatestYear)
			VAR IsTwoMonthsAgo = (CurrentMonth = LatestMonth - 2 || (LatestMonth = 2 && CurrentMonth = 12) && CurrentYear = LatestYear)
			
			RETURN 
			IF(IsLatestMonth || IsPreviousMonth || IsTwoMonthsAgo, 1, 0)
			```
		formatString: 0
		lineageTag: 9d4d22f6-8f55-4ee6-b971-920e77a92343
		summarizeBy: sum

		annotation SummarizationSetBy = Automatic

	column IsLast6Months =
			
			IF(
			    'Date_Table'[Date] >= EDATE(TODAY(), -6) && 'Date_Table'[Date] <= TODAY(),
			    1,
			    0
			)
		formatString: 0
		lineageTag: 84164502-36f3-4bcc-ab1f-a63526ac9b4b
